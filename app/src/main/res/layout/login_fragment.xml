<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginFragment">
<TableLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent" android:layout_margin="2dp">
    <TableRow android:weightSum="1" android:layout_width="match_parent">
        <EditText android:id="@+id/tbCodigoOperario" android:background="@drawable/borde" android:layout_weight="1" android:textSize="50dp"></EditText>
    </TableRow>
    <TableRow android:weightSum="3" android:layout_height="wrap_content">
        <info.hoang8f.widget.FButton android:onClick="clickNumero" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="1" android:textSize="30dp"></info.hoang8f.widget.FButton>
        <info.hoang8f.widget.FButton android:onClick="clickNumero" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="2" android:textSize="30dp"></info.hoang8f.widget.FButton>
        <info.hoang8f.widget.FButton android:onClick="clickNumero" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="3" android:textSize="30dp"></info.hoang8f.widget.FButton>
    </TableRow>
    <TableRow android:weightSum="3" android:layout_height="wrap_content">
        <info.hoang8f.widget.FButton android:onClick="clickNumero" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="4" android:textSize="30dp"></info.hoang8f.widget.FButton>
        <info.hoang8f.widget.FButton android:onClick="clickNumero" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="5" android:textSize="30dp"></info.hoang8f.widget.FButton>
        <info.hoang8f.widget.FButton android:onClick="clickNumero" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="6" android:textSize="30dp"></info.hoang8f.widget.FButton>
    </TableRow>
    <TableRow android:weightSum="3" android:layout_height="wrap_content">
        <info.hoang8f.widget.FButton android:onClick="clickNumero" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="7" android:textSize="30dp"></info.hoang8f.widget.FButton>
        <info.hoang8f.widget.FButton android:onClick="clickNumero" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="8" android:textSize="30dp"></info.hoang8f.widget.FButton>
        <info.hoang8f.widget.FButton android:onClick="clickNumero" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="9" android:textSize="30dp"></info.hoang8f.widget.FButton>
    </TableRow>
    <TableRow android:weightSum="3" android:layout_height="wrap_content">
        <info.hoang8f.widget.FButton android:onClick="borrar" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="&lt;" android:textSize="30dp" android:backgroundTint="@color/red"></info.hoang8f.widget.FButton>
        <info.hoang8f.widget.FButton android:onClick="clickNumero" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="0" android:textSize="30dp"></info.hoang8f.widget.FButton>
        <info.hoang8f.widget.FButton android:onClick="ok" android:layout_margin="1dp" android:layout_weight="1" android:layout_height="100dp" android:layout_width="100dp" android:text="OK" android:textSize="30dp" android:backgroundTint="@color/green"></info.hoang8f.widget.FButton>
    </TableRow>
</TableLayout>
</androidx.constraintlayout.widget.ConstraintLayout>